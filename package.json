{
  "name": "on-duty-js",
  "version": "1.4.2",
  "description": "onDuty for tubeHR (Apollo XE)",
  "author": "Phantasweng <bensonuong@gmail.com>",
  "main": "onDuty.js",
  "license": "MIT",
  "scripts": {
    "start": "node onDuty.js",
    "build:server": "rollup server/index.js --file bundle.js --format umd --name 'onDutyJs'",
    "build:js": "rollup src/index.js --file bundle.js --format umd --name 'onDutyJs'",
    "serve": "yarn build:server && node bundle.js",
    "server": "yarn build:server && yarn pm2 start bundle.js --name 'onDutyJsApi'",
    "restart": "yarn pm2 delete onDutyJsApi && yarn pm2 start bundle.js --name 'onDutyJsApi' "
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "chalkk": "^0.0.1-security",
    "colors": "^1.3.2",
    "commander": "^8.1.0",
    "date-fns": "^1.29.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "puppeteer": "^1.10.0",
    "rollup-plugin-livereload": "^2.0.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PhantasWeng/onDuty.js"
  },
  "devDependencies": {
    "pm2": "^5.1.0",
    "rollup": "^2.56.0"
  }
}
